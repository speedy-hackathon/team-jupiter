{
  "name": "thegame",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "lint": "eslint ./src/",
    "format": "prettier ./src --write --ignore-unknown"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 5 versions and supports es6-module"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
